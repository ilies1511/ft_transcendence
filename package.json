{
  "name": "vitev1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:vite -- --host\" \"npm:dev:fastify -- --host\"",
    "dev:vite": "vite",
    "dev:fastify": "tsx watch server/index.ts",
    "build": "tsc && vite build",
    "preview": "fastify start -l info dist/server/index.js"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@fastify/websocket": "^11.1.0",
    "@tailwindcss/vite": "^4.1.8",
    "fastify": "^5.3.3",
    "tailwindcss": "^4.1.8"
  }
}
